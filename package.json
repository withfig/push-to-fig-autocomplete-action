{
  "name": "push-to-fig-autocomplete-action",
  "version": "2.3.0",
  "private": true,
  "description": "Action to automatically open a new PR to the withfig/autocomplete repo",
  "main": "lib/main.js",
  "packageManager": "pnpm@9.1.4",
  "scripts": {
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "esbuild --bundle src/main.ts --outfile=dist/index.js --minify --keep-names --sourcemap --platform=node",
    "test": "vitest --run --passWithNoTests",
    "all": "npm run format && npm run lint && npm run package && npm test",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">= 20"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/withfig/push-to-fig-autocomplete-action.git"
  },
  "keywords": [
    "fig",
    "spec",
    "autocomplete",
    "actions",
    "node",
    "setup"
  ],
  "author": "Grant Gurvis <grangurv@amazon.com>",
  "license": "MIT",
  "dependencies": {
    "@actions/artifact": "^2.1.4",
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "@fig/autocomplete-merge": "^1.3.0"
  },
  "devDependencies": {
    "@types/eslint": "^8.56.5",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "esbuild": "^0.21.4",
    "eslint": "^8.57.0",
    "eslint-plugin-github": "5.0.0-2",
    "husky": "^9.0.11",
    "js-yaml": "^4.1.0",
    "prettier": "^3.3.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "vitest": "^1.6.0"
  }
}
