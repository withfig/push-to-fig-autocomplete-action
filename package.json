{
  "name": "push-to-fig-autocomplete-action",
  "version": "1.0.0-alpha.0",
  "private": true,
  "description": "Action to automatically open a new PR to the withfig/autocomplete repo",
  "main": "lib/main.js",
  "scripts": {
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "webpack",
    "test": "jest --passWithNoTests",
    "all": "npm run format && npm run lint && npm run package && npm test"
  },
  "engines": {
    "node": ">= 16"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/withfig/push-to-fig-autocomplete-action.git"
  },
  "keywords": [
    "fig",
    "spec",
    "autocomplete",
    "actions",
    "node",
    "setup"
  ],
  "author": "Federico Ciardi <fed.ciardi@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@actions/artifact": "^1.0.0",
    "@actions/core": "^1.6.0",
    "@actions/github": "^5.0.0",
    "@fig/autocomplete-merge": "^1.1.0",
    "prettier": "^2.5.1"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.1",
    "@types/node": "^16.11.26",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^8.10.0",
    "eslint-plugin-github": "^4.3.5",
    "eslint-plugin-jest": "^26.1.1",
    "jest": "^27.2.5",
    "js-yaml": "^4.1.0",
    "ts-jest": "^27.1.2",
    "ts-loader": "^9.2.7",
    "typescript": "^4.6.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  }
}
